{
    "tracking_plan_file": "tracking_plan.json",
    "log_config": {
        "version": 1,
        "formatters": {
            "simple": {
                "format": "%(asctime)s | %(name)s | %(levelname)s | %(message)s"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "stream": "ext://sys.stdout",
                "formatter": "simple"
            },
            "file": {
                "class": "logging.FileHandler",
                "level": "DEBUG",
                "filename": "logs/gaunit.log",
                "formatter": "simple"
            }
        },
        "loggers": {
            "gaunit": {
                "level": "WARNING"
            }
        },
        "root": {
            "handlers": [
                "console",
                "file"
            ]
        }
    }
}